#include<iostream.h>
using namespace std;
int main()
  {
    float p,c,m,h,e,avg;
    cout<<"Enter marks in physics\n";
    cin>>p;
    cout<<"Enter marks in chemistry\n";
    cin>>c;
    cout<<"Enter marks in math\n";
    cin>>m;
    cout<<"Enter marks in hindi\n";
    cin>>h;
    cout<<"Enter marks in english\n";
    cin>>e;
    avg=(p+c+m+h+e)/5;
    cout<<"Average of result="<<avg;
  }
