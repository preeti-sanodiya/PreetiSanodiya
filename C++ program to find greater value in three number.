#include<iostream.h>
using namespace std;
int main()
  {
    int a,b,c;
    cout<<"Enter first number\n";
    cin>>a;
    cout<<"Enter second number\n";
    cin>>b;
    cout<<"Enter third number\n";
    cin>>c;
    if(a>b&&a>c)
    cout<<a<<" is greater";
    if(b>a&&b>c)
    cout<<b<<" is greater";
    if(c>a&&c>b)
    cout<<c<<" is greater";    
}
